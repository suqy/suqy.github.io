<!doctype html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta name="description" content="Content"/>
    <meta name="keywords" content="suqin,前端"/>
    <meta name="author" content="suqin"/>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1"/>
    <title>suqin index</title>
    <link rel="stylesheet" href="/assets/themes/default/main.css"/>
    <link rel="icon" href="../assets/images/icon.png"/>
</head>
<body>
    <div class="main">
        <div class="header">
            <div class="hdimg a-fadeinB">
                <a href=""><img src="assets/images/psb.jpg" alt=""/></a>
            </div>
            <h1 class="tit">SuQin</h1>
            <p class="link"><a href="/pages/about-me.html" target="_self">关于我</a> <a href="/tag.html">文章归档</a> <a href="https://github.com/ericzhang-cn/papery">Github</a></p>
        </div>
        <div id="article-blocks">
            <% articles.forEach(function (ar) { %>
            <div class="article-block clearfix">
                <div class="side">
                    <a href="javascript:void(0);" class="year"><%= ar.postedOn %></a>
                    <span class="data"><%= ar.data %></span>
                </div>
                <div class="content">
                    <p class="title"><a href="/articles/<%= ar.id %>.html"><%= ar.title %></a></p>
                    <p class="img"><a href="/articles/<%= ar.id %>.html"><%- ar.img %></a></p>
                    <p class="abstract"><%= ar.abstract %></p>
                </div>
            </div>
            <% }) %>
        </div>
    </div>
    <div class="footer">
        <div class="friend-links">友情链接 :
            <% ext.friendlink.forEach(function (fl) { %>
            <a href="<%= fl.url%>" target="_blank"><%= fl.label %></a>
            <% }) %>
        </div>
        <p> © 2015 suqin</p>
    </div>
</body>
</html>

